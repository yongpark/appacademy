<!DOCTYPE html>
<html>
  <head>
    <title>MusicApp</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>



  <body>
      <% if flash[:errors] %>
       <ul>
       <% flash[:errors].each do |error| %>
         <li><%= error %></li>
       <% end %>
       </ul>
     <% end %>
    <%= yield %>
  </body>
</html>
